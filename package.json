{
  "name": "eslint-plugin-coffee",
  "version": "0.0.11",
  "description": "ESLint plugin for Coffeescript",
  "main": "lib/index.js",
  "scripts": {
    "test": "node_modules/mocha/bin/_mocha lib/tests/**/*.js",
    "watch": "coffee -o lib/ -wc src/",
    "build": "coffee -o lib/ -c src/",
    "prepublish": "npm run build",
    "lint": "eslint src/*.coffee src/*/*.coffee src/*/*/*.coffee",
    "lint-fix": "eslint --fix src/*.coffee src/*/*.coffee src/*/*/*.coffee",
    "lint-js": "eslint src/*.coffee src/*/*.coffee -c .eslintrc-js.yml"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "eslint",
    "coffeescript",
    "eslint-plugin",
    "eslintplugin"
  ],
  "author": "Julian Rosse <@helixbass>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-coffee": "^0.0.11",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.3.0",
    "mocha": "^5.2.0",
    "prettier": "github:helixbass/prettier#9105f5f1",
    "prettier-plugin-coffeescript": "^0.0.9"
  },
  "peerDependencies": {
    "eslint": ">=4.14.0"
  },
  "dependencies": {
    "babel-eslint": "^7.2.2",
    "babylon": "^7.0.0-beta.44",
    "coffeescript": "github:helixbass/coffeescript#ff2b38d4",
    "doctrine": "^2.1.0",
    "eslint-scope": "~3.7.3",
    "eslint-utils": "^1.3.1",
    "eslint-visitor-keys": "^1.0.0",
    "jsx-ast-utils": "^2.0.1",
    "lodash": "^4.17.10"
  }
}
