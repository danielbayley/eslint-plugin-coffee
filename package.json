{
  "name": "eslint-plugin-coffee",
  "version": "0.0.7",
  "description": "ESLint plugin for Coffeescript",
  "main": "lib/index.js",
  "scripts": {
    "test": "node_modules/mocha/bin/_mocha lib/tests/**/*.js",
    "watch": "coffee -o lib/ -wc src/",
    "lint": "eslint src/*.coffee src/*/*.coffee",
    "lint-js": "eslint src/*.coffee src/*/*.coffee -c .eslintrc-js.yml"
  },
  "keywords": [
    "eslint",
    "coffeescript",
    "eslint-plugin",
    "eslintplugin"
  ],
  "author": "Julian Rosse <@helixbass>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "github:helixbass/eslint#967ef170",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-coffee": "^0.0.4",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "prettier": "github:helixbass/prettier#f5366a5",
    "prettier-plugin-coffeescript": "^0.0.2"
  },
  "peerDependencies": {
    "eslint": ">=4.14.0"
  },
  "dependencies": {
    "babel-eslint": "^7.2.2",
    "babylon": "^7.0.0-beta.44",
    "coffeescript": "github:helixbass/coffeescript#c08362a6",
    "eslint-scope": "~3.7.3",
    "eslint-utils": "^1.3.1",
    "eslint-visitor-keys": "^1.0.0",
    "lodash": "^4.17.10"
  }
}
